@model List<string>


<div class="container mt-5">
    @if (TempData["Success"] != null)
    {
        <script>
            Swal.fire({
                icon: 'success',
                title: 'Success!',
                text: '@TempData["Success"]',
                confirmButtonText: 'OK'
            });
        </script>
    }
    <div class="row">
        <div class="col-md-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Group Names</h4>
                    <ul class="list-group">
                        @foreach (var groupName in Model)
                        {
                            <li class="list-group-item"><a class="text-decoration-none text-black" asp-action="detail" asp-route-groupName="@groupName">@groupName</a></li>
                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

