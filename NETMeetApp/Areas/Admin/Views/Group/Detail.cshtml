@model List<NETMeetApp.Models.AppUser>

<div class="container mt-5">
    <div class="row">
        <div class="col-md-12 grid-margin stretch-card">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Students in Group: @ViewBag.GroupName</h4>
                    <ul class="list-group">
                        @foreach (var student in Model)
                        {
                            <li class="list-group-item">
                                <strong>@student.FullName</strong> (@student.UserName) - @student.Email
                            </li>
                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
